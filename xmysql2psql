require 'rubygems'

version = ">= 0"

if ARGV.first =~ /^_(.*)_$/ and Gem::Version.correct? $1 then
  version = $1
  ARGV.shift
end

gem 'mysql2psql', version
load Gem.bin_path('mysql2psql', 'mysql2psql', version)
